      "next/babel",
      "es2015-riot",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          },
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "react-hot-loader/babel",
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --config webpack/webpack.dev.babel.js --mode development",
    "build": "webpack --config webpack/webpack.pro.babel.js --mode production",
    "ssr-dev": "node server.js",
    "ssr-build": "next build",
    "ssr-start": "NODE_ENV=production node server.js",
    "ssr-test": "next",
    "gulp": "gulp",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(babel-|@babel|next)).*$/' -a"
  }
